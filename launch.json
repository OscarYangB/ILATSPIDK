{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Debug ILATSPIDK",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/ILATSPIDK.exe",
            "args": ["-q"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Import pretty-printing for gdb",
					"text": "python import sys;sys.path.insert(0, 'C:\\pretty_printer');from printers import register_libcxx_printer_loader;register_libcxx_printer_loader()",
					"ignoreFailures": false
				},
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": false
				}
			]
        }
    ]
}
